# Database settings
POSTGRES_USER=morpheus_user
POSTGRES_PASSWORD=morpheus_password
POSTGRES_DB=morpheus_db
DATABASE_URL=postgresql+asyncpg://morpheus_user:morpheus_password@localhost:5433/morpheus_db

# Redis settings
REDIS_PASSWORD=your_redis_password
REDIS_URL=redis://:your_redis_password@localhost:6379/0

# JWT settings
JWT_SECRET_KEY=super_secret_key_change_me
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# CORS (comma-separated URLs for allowed origins, leave empty for development)
BACKEND_CORS_ORIGINS=

# Automation Feature
AUTOMATION_FEATURE_ENABLED=true

# AWS KMS (for production)
KMS_PROVIDER=aws
KMS_MASTER_KEY_ID=your_kms_key_id_or_arn
AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=your_access_key_id         # If not using IAM roles
# AWS_SECRET_ACCESS_KEY=your_secret_access_key # If not using IAM roles

# Development mode local encryption
MASTER_ENCRYPTION_KEY=generate_this_with_openssl_rand_-hex_32

# Proxy Router URL and authentication credentials
PROXY_ROUTER_URL=http://localhost:8082
PROXY_ROUTER_USERNAME=admin
PROXY_ROUTER_PASSWORD=admin

